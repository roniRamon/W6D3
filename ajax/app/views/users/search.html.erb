<h1>Search!</h1>

<nav class="users-search">
  <label>Search for User:
    <input class="user-search-input" type="text" value=""/>
  </label>

  <ul class="users">

  </ul>
</nav>

<ul>
  <% @users.each do |user| %>
    <li>
      <%= link_to user.username, user_url(user) %>
      <%= render 'follows/form', user: user %>
    </li>
  <% end %>
</ul>
